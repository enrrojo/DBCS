{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-RestApplication<REST>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.uva.REST.RestApplication",
            "projectName": "REST",
            "args": "",
